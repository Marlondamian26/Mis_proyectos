    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/qrcode": "^1.5.5",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "cache-manager": "^6.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
